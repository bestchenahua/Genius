<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/linearLayout14"
    android:layout_width="match_parent"
    android:background="@drawable/border_rad8_white"
    android:layout_height="wrap_content"
    android:layout_margin="6dp"

    android:elevation="5dp">


    <CheckBox
        android:id="@+id/checkBox2"
        android:drawablePadding="7dp"
        android:button="@drawable/checkbox_style3"
        android:text=""
        android:textColor="#2DB7B5"
        android:textSize="13sp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="15dp"
        android:paddingEnd="14dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        />

    <ImageView
        android:id="@+id/imageView83"
        android:layout_width="16dp"
        android:layout_height="14dp"
        android:layout_marginTop="18dp"
        app:layout_constraintStart_toEndOf="@+id/checkBox2"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_circle_unselect" />

    <TextView
        android:id="@+id/textView104"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="9dp"
        android:layout_marginTop="14dp"
        android:text=""
        android:textSize="14sp"
        android:textColor="#515151"
        app:layout_constraintStart_toEndOf="@+id/imageView83"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView82"
        android:layout_width="5dp"
        android:layout_height="8dp"
        android:layout_marginStart="9dp"
        android:layout_marginTop="20dp"
        app:layout_constraintStart_toEndOf="@+id/textView104"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic1" />

    <android.support.v7.widget.RecyclerView
        android:id="@+id/recyclerView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:layout_marginBottom="15dp"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView104" />
</android.support.constraint.ConstraintLayout>
